  "description": "Reads ini-file style size values into bytes",
  "homepage": "https://github.com/vebut/ini-size-reader",
  "license": "MIT",

  "minimum-stability": "stable",
  "keywords": [
    "ini",
    "size",
    "byte",
    "reader",
    "converter"
  ],

  "autoload": {
    "psr-4": {
